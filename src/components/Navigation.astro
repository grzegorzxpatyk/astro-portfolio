---
import NavLinks from './NavLinks';

interface Props {
    routes: string[];
}

const { routes } = Astro.props;
const pathname = Astro.url.pathname;
---

<nav class='flex flex-row justify-start items-start h-[5dvh] gap-4 text-2xl'>
    <NavLinks {...{ routes, pathname }} client:load transition:persist />
</nav>
